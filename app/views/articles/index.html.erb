<h1>Articles</h1>
<a href="/articles/new">New article</a>

<ul>
    <% @articles.each do |article| %>
        <li>
            <h2>
                <a href=<%= "/articles/#{article.id}" %>><%= article.title %></a>
            </h2>
            <p><%= article.content %></p>
        </li>
    <% end %>
</ul>

